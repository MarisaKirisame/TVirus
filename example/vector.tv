let main = 
  let a = 999 in
  let b = 997 in
  let c = 21 in
  let v1 = vec_build(\x . x, 100000) in
  let v2 = vec_modify(v1, a, \x . vec_access(v1, b)) in
  let v3 = vec_modify(v1, a, \x . vec_access(v1, c)) in
  let v4 = vec_zipWith(v1, v2, (\a, b . (a - b))) in
  let v5 = vec_zipWith(v3, v4, (\a, b . (a * b)))
  in vec_access(v5, a)
